<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="kr.co.main.admin.AdminMapper">
	<select id="adminLoginProcess" parameterType="kr.co.main.admin.AdminMapper" resultType="Integer">
		SELECT COUNT(*) FROM wego.admin WHERE id = #{id} AND pwd = #{pwd}
	</select>
</mapper>




